{
    "name": "wyrihaximus/react-event-loop-rx-scheduler-hook-up",
    "description": "\ud83e\ude9d Hook up ReactPHP Event Loop to the RxPHP Scheduler",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "react/event-loop": "^1.3",
        "reactivex/rxphp": "^2.0"
    },
    "require-dev": {
        "wyrihaximus/async-test-utilities": "^5.0.25"
    },
    "conflict": {
        "azjezz/psl": "<2"
    },
    "autoload": {
        "psr-4": {
            "WyriHaximus\\React\\": "src/"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "ergebnis/composer-normalize": true,
            "icanhazstring/composer-unused": true,
            "infection/extension-installer": true
        },
        "platform": {
            "php": "8.1.0"
        },
        "sort-packages": true
    },
    "extra": {
        "unused": [
            "wyrihaximus/react-mutex",
            "wyrihaximus/react-mutex-contracts"
        ]
    },
    "scripts": {
        "post-install-cmd": [
            "composer normalize"
        ],
        "post-update-cmd": [
            "composer normalize"
        ]
    }
}
